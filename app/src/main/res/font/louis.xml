<?xml version="1.0" encoding="utf-8"?>
<font-family xmlns:android="http://schemas.android.com/apk/res/android"
    xmlns:app="http://schemas.android.com/apk/res-auto">

    <!-- regular -->
    <font
        app:font="@font/louis_regular"
        app:fontStyle="normal"
        app:fontWeight="400" />

    <!-- italic -->
    <font
        app:font="@font/louis_italic"
        app:fontStyle="italic"
        app:fontWeight="400" />

    <!-- bold -->
    <font
        app:font="@font/louis_bold"
        app:fontStyle="normal"
        app:fontWeight="700" />


    <!-- bold Italic -->
    <font
        app:font="@font/louis_bold_italic"
        app:fontStyle="normal"
        app:fontWeight="700" />

</font-family>